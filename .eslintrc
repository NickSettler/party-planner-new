{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": ["react-app", "plugin:prettier/recommended", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "@typescript-eslint/ban-ts-comment": 0,
    "quotes": [2, "double", { "avoidEscape": true }]
  },
  "globals": {
    "self": true
  }
}
